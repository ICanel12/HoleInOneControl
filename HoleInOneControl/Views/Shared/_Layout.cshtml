  <div class="content-wrapper">

    <section class="content-header">
      <div class="container-fluid">
        <div class="row mb-2">
          <div class="col-sm-6">
           
          </div>
          <div class="col-sm-6">

          </div>
        </div>
      </div><!-- /.container-fluid -->
    </section>

      <section class="content">
      <div class="container-fluid">
        <div class="row">
          <div class="col-12">
              <div class="card">
                   @RenderBody()
              </div>
            
           </div>
        </div>
                </div>
        </section>
    </div>
    <!-- /.content -->
  </div>
  <!-- /.content-wrapper -->
